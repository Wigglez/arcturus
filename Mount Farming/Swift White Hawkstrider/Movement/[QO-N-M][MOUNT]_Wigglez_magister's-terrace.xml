			<CustomBehavior File="Message" 
							Text="We are in Eastern Kingdoms." 
							LogColor="Cyan" />
							
			<CustomBehavior File="Message" 
							Text="Flying to Light's Hope Chapel." 
							LogColor="Lime" />
			
			<CustomBehavior File="FlyTo" X="2270.2" Y="-5343.11" Z="145.3869" />

			<!-- Fly down -->
			<CustomBehavior File="FlyTo" X="2270.2" Y="-5343.11" Z="86.9733" />
